<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.navigation2_leretour">

    <application
        android:allowBackup="true"
        android:icon="@drawable/saadoeyer"
        android:label="@string/app_name"
        android:roundIcon="@drawable/saadoeyer"
        android:supportsRtl="true"
        android:theme="@style/Theme.Navigation2_LeRetour">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Navigation2_LeRetour.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Connexion"
            android:label="Connexion"
            android:theme="@style/Theme.Navigation2_LeRetour.NoActionBar">

        </activity>
    </application>

</manifest>